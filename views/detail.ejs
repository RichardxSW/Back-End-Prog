        <div class="recipe">
            <div class="recipe-image">
                <img src=" <%= recipes.img %>" alt="Gambar Nasgor">
                <div class="desc">
                    <%= recipes.desc %>
                </div>

                    <div class="review-box">
                        <div class="review-box-title">
                            <h1>Reviews</h1>
                        </div>
                        <div class="review-name">
                                <img src="<%= recipes.img %>" alt="gbr">
                            <span class="name-text">
                                <%= recipes.title %>
                            </span>
                        </div>
                        <div class="rating">
                            <span class="rating-title">
                                Your Rating
                            </span>
                            <div class="rating-star">
                                <div class="rating-text">
                                    <span class="rtext"></span>
                                </div>
                                <input type="radio" id="star5" name="rating" value="5"><label for="star5"></label>
                                <input type="radio" id="star4" name="rating" value="4"><label for="star4"></label>
                                <input type="radio" id="star3" name="rating" value="3"><label for="star3"></label>
                                <input type="radio" id="star2" name="rating" value="2"><label for="star2"></label>
                                <input type="radio" id="star1" name="rating" value="1"><label for="star1"></label>
                            </div>
                        </div>
                        <div class="review">
                            <span class="review-title">
                                Your Review
                            </span>
                            <textarea class="review-textarea" rows="4" cols="40" placeholder="What do you think about this recipe?"></textarea>
                            <button class="submit-button">SUBMIT</button>
                        </div>
                    </div>

                    <% for (let i = 0; i < recipes.rating.length; i++) { %>
                    <div class="submission-container">
                            <div class="sub-img-name">
                                <img class="sub-img" src="<%= recipes.photo[i] %>">
                                <div class="sub-name"><%= recipes.name[i] %></div>
                            </div>
                            <div class="rating-container">
                                <% for (let j = 0; j < 5; j++) { %>
                                <span class="sub-rating-date">&#9734;</span>
                                <% } %>
                                
                                <span class="sub-date"><%= recipes.date[i] %></span>
                            </div>
                            <p class="sub-review">
                                <%= recipes.review[i] %>
                            </p>
                    </div>
                    <% } %>
            </div>

            <div class="recipe-info">
                <div class="recipe-title">
                    <div class="foodName"><%= recipes.title %></div>

                    <button class="unsaved">
                        <span class="iconUnsaved"><i class="fa-regular fa-bookmark"></i></span>
                        <span class="save-text">Pin</span>
                    </button>

                    <button class="saved">
                        <span class="iconSaved"><i class="fa-solid fa-bookmark"></i></span>
                        <span class="unsaved-text">Unpin</span>
                    </button>
                </div>  

                <div class="detail-stats">

                    <div class="stats-item">
                        <span class="ingr"><%= recipes.length %></span>

                        <span class="label-medium">Ingredients</span>
                    </div>

                    <div class="stats-item">
                        <span class="mnt"><%= recipes.minutes %></span>

                        <span class="label-medium">Minutes</span>
                    </div>

                    <div class="stats-item">
                        <span class="clr"><%= recipes.calories %></span>

                        <span class="label-medium">Calories</span>
                    </div>
                </div>

                <div class="tag-list">
                    <% recipes.category.forEach(category => { %>
                        <div class="filter-chip"><%= category %></div>
                    <% }) %>
                </div>

                <details>
                    <summary>
                        <span class="ingr-title">Ingredients</span>
                        <div class="serving-text">for <span class="serving"><%= recipes.serving %></span> serving</span></div>
                    </summary>

                    <ul class="ingr-list">
                        <% recipes.bahan.forEach(function(ingr) { %>
                            <li class="ingr-item"><%= ingr %></li>
                        <% }); %>

                        <!-- // <li class="ingr-item">Rice</li>
                        // <li class="ingr-item">Garlic</li>
                        // <li class="ingr-item">Egg</li>
                        // <li class="ingr-item">Sweet Soy Sauce</li>
                        // <li class="ingr-item">Salt</li>
                        // <li class="ingr-item">White Pepper</li> -->
                    </ul>
                </details>
                
                <details>
                    <summary>
                        <span class="inst-title">Instructions</span>
                    </summary>

                    <ol class="inst-list">
                        <% recipes.cara.forEach(function(inst) { %>
                            <li class="inst-item"><%= inst %></li>
                        <% }); %>

                        <!-- <li class="inst-item">Heat oil over medium heat.</li>
                        <li class="inst-item">Saute spices until fragrant.</li>
                        <li class="inst-item">Add rice and egg, stir until smooth.</li>
                        <li class="inst-item">Add sweet soy sauce, stir again until smooth. </li>
                        <li class="inst-item">Remove from heat and serve with complementary fried rice. </li> -->
                    </ol>
                </details>

            </div>
        </div>
<script>
    const recipes = `<%- JSON.stringify(recipes) %>`;
</script>   
<script>
    const picValue = "<%= pic %>";
    const nameValue = "<%= name %>";
</script>

<script src="/js/search.js"></script>
<script src="/js/btnDetail.js"></script>
<script src="/js/detail.js"></script>
